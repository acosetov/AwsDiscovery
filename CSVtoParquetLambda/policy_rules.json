{
    "Version": "2012-10-17",
    "Statement": [
     {
      "Effect": "Allow",
      "Action": [
       "logs:PutLogEvents",
       "logs:CreateLogGroup",
       "logs:CreateLogStream"
      ],
      "Resource": "arn:aws:logs:*:*:*"
     },
     {
      "Effect": "Allow",
      "Action": [
       "s3:*"
      ],
      "Resource": [
       "arn:aws:s3:::csv-bucket-act24/*",
       "arn:aws:s3:::csv-bucket-act24",
       "arn:aws:s3:::parquet-bucket-act24/*",
       "arn:aws:s3:::parquet-bucket-act24"
      ]
     }
    ]
   }